<?xml version="1.0" encoding="utf-8"?>
<ontology name="FoundItOntology">

    <sort name="field" dynamic="true"/>
    <sort name="country" dynamic="true"/>
    <sort name="city" dynamic="true"/>
    <sort name="contract" dynamic="true"/>
    <sort name="extra_info" dynamic="true"/>

    <predicate name="field_to_search" sort="field"/>
    <predicate name="country_to_search" sort="country"/>
    <predicate name="city_to_search" sort="city"/>
    <predicate name="contract_to_search" sort="contract"/>
    <predicate name="extra_info_to_search" sort="extra_info"/>

    <predicate name="job_seeker" sort="string"/>
    <action name="job_application"/>

    <individual name="field_entries" sort="field"/>
    <individual name="country_entries" sort="country"/>
    <individual name="city_entries" sort="city"/>
    <individual name="contract_entries" sort="contract"/>
    <individual name="extra_info_entries" sort="extra_info"/>

</ontology>
